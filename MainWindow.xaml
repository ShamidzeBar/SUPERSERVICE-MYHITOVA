<Window x:Class="SUPERSERVICE_MYHITOVA.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SUPERSERVICE_MYHITOVA"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Image Source="Resources/school_logo.png" Width="70" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5" />
        <Frame Name="MainFrame" ContentRendered="MainFrame_ContentRendered"></Frame>
        <Button Name="BackBtn" Click="BackBtn_Click" Content="Назад" HorizontalAlignment="Right" VerticalAlignment="Top" Width="150" Height="30" Margin="10"></Button>
    </Grid>
</Window>
